INIT : MOV B,#00H
	   MOV R1,#00H
	   MOV R7,#02H
	   MOV A,#01H
       MOV R6, #08H
SJMP START
START : CPL A
	    MOV P1,A
	    CALL DELAY
	    CPL A
	    MOV P1,A
	    MOV B,R7
	    MUL AB
		DJNZ R6, START
SJMP INIT
DELAY : MOV R3,#01
LOOP1 : MOV R4,#255
LOOP2 : MOV R5,#255
LOOP3 : DJNZ R5,LOOP3
	    DJNZ R4,LOOP2
	    DJNZ R3,LOOP1
RET 

